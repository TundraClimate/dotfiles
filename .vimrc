set backspace=indent,eol,start
set nocompatible
set expandtab
set tabstop=4
set shiftwidth=0

inoremap <silent> jj <Esc>

nnoremap H 10h
nnoremap J 10j
nnoremap K 10k
nnoremap L 10l
nnoremap v, ggVG
inoremap <silent> <C-j> <C-n>
inoremap <silent> <C-k> <C-p>
nnoremap <silent> <Right> gt
inoremap <silent> <Right> gt
